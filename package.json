{
  "name": "react-sky-ui",
  "version": "1.0.0",
  "description": "learn react ui",
  "keywords": [
    "react-component",
    "ui"
  ],
  "homepage": "https://github.com/NEmperor/react-sky-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/NEmperor/react-sky-ui"
  },
  "files": [
    "lib",
    "es"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "dll": "webpack --config ./config/webpack.dll.js",
    "eslint": "eslint src --ext .js,.ts,.tsx",
    "build-ts": "tsc -p tsconfig.build.json",
    "build:lib": "cross-env OUTPUT_MODULE=commonjs babel src/components -d lib",
    "build:es": "babel src/components -d es",
    "build:css": "gulp"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^4.6.5",
    "babel-plugin-import": "^1.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "connected-react-router": "^6.8.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "immer": "^8.0.1",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.13.0",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpackbar": "^4.0.0"
  }
}
